#include<stdio.h>
void main(){
  int a,b;
  printf("enter two nums:");
  scanf("%d%d",&a,&b);
  char op;
  printf("\n enter symbol:");
  scanf("%c,&op);
  switch(op)
{
   case'+':
     printf("the sum is %d",a+b);
     break;
   case'-':
     printf("the difference is %d",a-b);
     break;
   case'*':
    printf("the multiple is %d",a*b);
     break;
   case'/':
    printf("the division is %d",a/b);
    break;
   default:
    printf("wrong choice");
  }
}
