#include<stdio.h>
void main(){
  int a;
  printf(enter any number:");
  scanf("%",&a);
  if(a%2==0)
  {
    for(int i=1;i<=10;i++)
    {
       printf("\n%d*%d=%d",i,a,i*a);
       i=i+1;
      }
     }
      else
      {
       for(int i=2;i<=15;i++)
       {
         printf("\n%d*%d=%d",i,a,i*a);
         i=i+1;
       }
     }
  }
